<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mpping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<!-- Hibernate mapping file, it maps a Java class Sport(ID,Sport) to database table sports(id,sports) -->

<hibernate-mapping>

	
	<class name="db.Laptop" table="laptop">
	
		<id name="product_id" type="string"/>
		<property name="connectivity" column="connectivity" type="string"/>
		<property name="RAM_type" column="RAM_type" type="string"/>
		<property name="RAM_capacity_GB" column="RAM_capacity_GB" type="string"/>
		

		</class>
</hibernate-mapping>